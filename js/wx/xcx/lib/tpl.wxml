<template name="odd">
    <view> odd - {{a}} == {{o.a}}</view>
</template>

<template name="even">
    <view> even</view>
</template>

<template name="header">
    <header class="flex"> even</header>
</template>

<template name="footer">
    <footer class="flex"> even</footer>
</template>

<template name="a">
    <include src="a.wxml"/>
</template>

<template name="c/d.e">
    ccc/ddd.eee = {{ccc}}

    <button bindtap="{{ccc+''}}">btn</button>
</template>

<template name="x">
    <button bindtap="{{handler}}">failed {{handler}}</button>
</template>

